{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run QEMU (RISC-V User-Mode)",
      "type": "shell",
      "command": "make open_dgb",
      "args": [],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "isBackground": true,
      "problemMatcher": {
        "owner": "qemu",
        "pattern": {
          "regexp": "."
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".",
          "endsPattern": "^$^"
        }
      }
    }
  ]
}
